{templateType Webovac\Core\Control\Breadcrumbs\BreadcrumbsTemplate}
<div n:if="(!isModuleInstalled('style') || $webData->layout !== 'default') && ($webData->layout !== 'simple' || count($crumbs) > 1)" class="breadcrumbs" n:inner-foreach="$crumbs as $crumb">
	<a href="{$crumb['link']}">{$crumb['title']|noescape}</a>
	<span n:sep class="separator"> : </span>
</div>
